<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE dwr PUBLIC "-//GetAhead Limited//DTD Direct Web Remoting 2.0//EN" "http://www.getahead.ltd.uk/dwr/dwr20.dtd">
<dwr>
	<allow>
		<create creator="spring" javascript="JUserCheck">
			<param name="beanName" value="UserBizimpl"/>
			<param name="location" value="applicationContext.xml"></param>
		</create>
		<convert converter="bean" match="com.entity.TBGPSDATA">
                  <param name="include" value="id,lat,lng,latoffset,lngoffset,speed,angle,gpstime,updatetime" />
        </convert>
        	<convert converter="bean" match="com.entity.TBGPSSHARE">
            <param name="include" value="id,lat,lng,name,userid,memo,bgntime,endtime" />
        </convert>
	</allow>
</dwr>